{
  "logging": {
    "console": false,
    "gcloud": true,
    "file": false
  },
  "firestore": null,
  "pubsub": {
    "client": null,
    "subscriptions": {}
  },
  "api": {
    "showError": false
  },
  "paypal": {
    "minFiscalYear": 2020,
    "validateIpn": true,
    "ipnUrl": "https://ipnpb.paypal.com/cgi-bin/webscr"
  },
  "chromium": {
    "maxInstances": 5
  },
  "providers": {
    "file": {
      "provider": "gcloud-storage",
      "gcloud": {
        "keyPath": null,
        "projectId": null,
        "translationsBucket": "auto-receipt-prod-refs",
        "templatesBucket": "auto-receipt-prod-refs",
        "documentsBucket": "auto-receipt-prod-documents",
        "tempBucket": "auto-receipt-prod-temp",
        "createBuckets": false
      }
    },
    "authentication": {
      "provider": "jwt",
      "unauthorizedStatusCode": 404
    },
    "email": {
      "provider": "smtp",
      "smtp": {
        "host": "smtp.gmail.com",
        "port": 465,
        "secure": true,
        "authUsername": "",
        "authPassword": "",
        "from": "reply@autoreceipt.app",
        "replyTo": "reply@autoreceipt.app"
      }
    }
  },
  "cors": {
    "enabled": true,
    "allowedOrigins": [
      "https://auto-receipt-prod.web.app",
      "https://auto-receipt-prod.firebaseapp.com"
    ]
  },
  "correspondence": {
    "enabled": true
  }
}
