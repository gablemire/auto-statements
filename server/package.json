{
  "name": "auto-receipt-server",
  "version": "1.2.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "build:watch": "tsc --watch --incremental --sourceMap",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run lint && tsc",
    "serve": "node --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "serve:watch": "nodemon --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "package:staging": "npm run build:prod && serverless package --config serverless.staging.yml",
    "deploy:staging": "npm run build:prod && serverless deploy --config serverless.staging.yml",
    "deploy:production": "npm run build:prod && serverless deploy --config serverless.production.yml",
    "start": "npm run build && run-p build:watch serve:watch"
  },
  "author": {
    "name": "Gabriel Lemire"
  },
  "contributors": [
    {
      "name": "Steven McKeown"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^4.2.0",
    "@google-cloud/logging-winston": "^4.0.0",
    "@google-cloud/pubsub": "^2.5.0",
    "@google-cloud/storage": "^5.3.0",
    "chrome-aws-lambda": "^5.2.1",
    "config": "^3.2.4",
    "csv-parse": "^4.8.5",
    "full-icu": "^1.3.0",
    "handlebars": "^4.7.2",
    "iltorb": "^2.4.4",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mjml": "^4.7.0",
    "nodemailer": "^6.4.17",
    "puppeteer-core": "^5.2.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "uuid": "^8.3.0",
    "winston": "^3.2.1",
    "winston-console-format": "^1.0.5"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.162",
    "@types/mjml": "^4.0.4",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.18",
    "@types/nodemailer": "^6.4.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/request-promise": "^4.1.45",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "paypal-ipn-types": "^1.0.0",
    "prettier": "^2.1.2",
    "puppeteer": "^5.2.1",
    "rimraf": "^3.0.0",
    "serverless": "^1.60.4",
    "serverless-google-cloudfunctions": "^3.1.1",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "^12.18.3"
  },
  "private": true
}
