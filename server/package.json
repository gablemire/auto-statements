{
  "name": "auto-receipt-server",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "build:watch": "tsc --watch --incremental --sourceMap",
    "prebuild:prod": "npm run clean",
    "build:prod": "npm run lint && tsc",
    "serve": "node --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "serve:watch": "nodemon --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "package:staging": "npm run build:prod && serverless package --config serverless.staging.yml",
    "deploy:staging": "npm run build:prod && serverless deploy --config serverless.staging.yml",
    "deploy:production": "npm run build:prod && serverless deploy --config serverless.production.yml",
    "start": "npm run build && run-p build:watch serve:watch"
  },
  "author": {
    "name": "Gabriel Lemire"
  },
  "contributors": [
    {
      "name": "Steven McKeown"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^3.0.0",
    "@google-cloud/logging-winston": "^3.0.2",
    "@google-cloud/pubsub": "^1.2.0",
    "@google-cloud/storage": "^4.3.0",
    "@hapi/joi": "^16.1.8",
    "@sendgrid/mail": "^6.5.1",
    "chrome-aws-lambda": "^2.0.2",
    "config": "^3.2.4",
    "full-icu": "^1.3.0",
    "handlebars": "^4.7.2",
    "iltorb": "^2.4.4",
    "puppeteer-core": "^2.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "winston-console-format": "^1.0.5"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.5",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.18",
    "@types/puppeteer-core": "^2.0.0",
    "@types/request-promise": "^4.1.45",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "paypal-ipn-types": "^1.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "serverless": "^1.60.4",
    "serverless-google-cloudfunctions": "^2.3.3",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": "10"
  },
  "private": true
}
