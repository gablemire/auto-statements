{
  "name": "auto-receipt-server",
  "version": "1.2.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "build:watch": "tsc --watch --incremental --sourceMap",
    "prebuild:prd": "npm run clean",
    "build:prd": "npm run lint && tsc",
    "serve": "node --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "serve:watch": "nodemon --icu-data-dir=node_modules/full-icu -r dotenv/config --inspect lib/index-local.js",
    "start": "npm run build && run-p build:watch serve:watch"
  },
  "author": {
    "name": "Gabriel Lemire"
  },
  "contributors": [
    {
      "name": "Steven McKeown"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^4.9.2",
    "@google-cloud/logging-winston": "^4.0.3",
    "@google-cloud/pubsub": "^2.8.0",
    "@google-cloud/storage": "^5.7.4",
    "archiver": "^5.2.0",
    "axios": "^0.21.1",
    "chrome-aws-lambda": "^7.0.0",
    "config": "^3.3.3",
    "csv-parse": "^4.15.1",
    "form-data": "^3.0.0",
    "full-icu": "^1.3.1",
    "handlebars": "^4.7.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mjml": "^4.8.1",
    "nodemailer": "^6.4.17",
    "puppeteer-core": "^7.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-console-format": "^1.0.8"
  },
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/mjml": "^4.7.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.19.16",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
